{
  "name": "react-native-phone-country-picker-input",
  "version": "1.0.7",
  "description": "A React Native component to input phone numbers with a country picker modal.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "devC": "expo start -c",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "build": "tsc && cpy src/components/caretdown.png dist/ --parents",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "expo",
    "expo-react-native",
    "expo-sdk53",
    "react-native",
    "phone-input",
    "country-picker",
    "phone-number",
    "component",
    "react-native-component",
    "typescript",
    "reactjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AtanuDebbarma/react-native-phone-country-picker-input.git"
  },
  "author": "Atanu Debbarma",
  "license": "MIT",
  "homepage": "https://github.com/AtanuDebbarma/react-native-phone-country-picker-input#readme",
  "dependencies": {
    "expo": "~53.0.9",
    "react": "19.0.0",
    "react-native": "0.79.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@react-native/eslint-config": "^0.79.2",
    "@types/react": "~19.0.10",
    "@typescript-eslint/typescript-estree": "^8.33.0",
    "babel-plugin-inline-import": "^3.0.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "prettier": "^3.5.3",
    "typedoc": "^0.27.9",
    "typescript": "^5.8.3"
  },
  "private": false
}